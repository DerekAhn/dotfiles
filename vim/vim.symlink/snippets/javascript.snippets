# Basic Snippets

snippet fn
	function ${1}(${2}) {
		${3}
	}

snippet afn
	function(${1}) {
		${2}
	}

snippet iife
	(function (${1}) {
		${2}
	})();

snippet int
	setInterval(function () {
		${2}
	}, ${1});

snippet sto
	setTimeout(function () {
		${2}
	}, ${1});

snippet cc
	console.log('${1}');

snippet ey
	eyes('${1}');

snippet rq
	require('${1}');

snippet me
	module.exports = ${1}
